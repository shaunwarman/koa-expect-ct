{
  "name": "koa-expect-ct",
  "description": "A koajs middleware for expect-ct header",
  "version": "0.0.1",
  "author": "Shaun Warman <shaunwarman1@gmail.com> (https://shaunwarman.com)",
  "bugs": {
    "url": "https://github.com/shaunwarman/koa-expect-ct/issues",
    "email": "shaunwarman1@gmail.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Shaun Warman <shaunwarman1@gmail.com> (https://shaunwarman.com)"
  ],
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ava": "latest",
    "codecov": "latest",
    "cross-env": "latest",
    "eslint": "latest",
    "eslint-config-xo-lass": "latest",
    "fixpack": "latest",
    "husky": "latest",
    "koa": "^2.11.0",
    "lint-staged": "latest",
    "nyc": "latest",
    "remark-cli": "latest",
    "remark-preset-github": "latest",
    "supertest": "^4.0.2",
    "xo": "latest"
  },
  "engines": {
    "node": ">=8.3"
  },
  "homepage": "https://github.com/shaunwarman/koa-expect-ct",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "koa-expect-ct",
    "lass"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "xo --fix",
      "git add"
    ],
    "*.md": [
      "remark . -qfo",
      "git add"
    ],
    "package.json": [
      "fixpack",
      "git add"
    ]
  },
  "main": "index.js",
  "nyc": {
    "check-coverage": true,
    "lines": 90,
    "functions": 90,
    "branches": 90,
    "reporter": [
      "lcov",
      "html",
      "text"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaunwarman/koa-expect-ct"
  },
  "scripts": {
    "ava": "cross-env NODE_ENV=test ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "xo && remark . -qfo",
    "lint:fix": "xo --fix",
    "nyc": "cross-env NODE_ENV=test nyc ava",
    "test": "npm run lint && npm run ava",
    "test-coverage": "npm run lint && npm run nyc"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ]
  }
}
